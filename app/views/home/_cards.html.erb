<% @coins.each do |coin| %>
  <div class="media col-sm-3 coin-card ">
    <img src="" alt="">
    <div class="media-body">
      <h5><%=coin.name %></h5>
      <p>
      <strong> Last Price</strong>  <%=number_to_currency(coin.last_price) %>
      </p>
      <p>
      <strong> Last update</strong> <%=coin.last_price_update %>
      </p>
       
       

    </div>

  </div>

<% end %>
