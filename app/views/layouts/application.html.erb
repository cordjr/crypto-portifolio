  <%= csrf_meta_tags %>
</head>
<body>
<nav class="navbar navbar-inverse">
   <div class="container-fluid">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapse" data-target="#bs-exampple-navbar-collapse1">
